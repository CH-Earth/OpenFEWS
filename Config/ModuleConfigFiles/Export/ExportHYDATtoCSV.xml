<timeSeriesExportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesExportRun.xsd">
    <export>
        <general>
            <exportType>generalCsv</exportType>
            <folder>$EXPORT_FOLDER$/WSC</folder>
            <exportFileName>
                <name>$LOOP_LOCATION_ID$_realtime_data_for_report.csv</name>
            </exportFileName>
            <table>
                <dateTimeColumn name="DATE" pattern="yyyy-MM-dd HH:mm"/>
                <locationColumn name="Location"/>
                <valueColumn name="Discharge" parameterId="Discharge"/>
                <valueColumn name="WaterLevel" parameterId="WaterLevel"/>
            </table>
            <idMapId>IdExportHYDAT</idMapId>
            <exportMissingValueString>-9999.0</exportMissingValueString>
             <columnSeparator>,</columnSeparator>
            <decimalSeparator>.</decimalSeparator>
        </general>
		<timeSeriesSet>
			<moduleInstanceId>ImportWSC</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>QR.obs</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="5"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportWSC</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>HG.obs</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="5"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
    </export>
        <export>
        <general>
            <exportType>generalCsv</exportType>
            <folder>$EXPORT_FOLDER$/WSC</folder>
            <exportFileName>
                <name>$LOOP_LOCATION_ID$_historical_data_for_report.csv</name>
            </exportFileName>
            <table>
                <dateTimeColumn name="DATE" pattern="yyyy-MM-dd HH:mm"/>
                <locationColumn name="Location"/>
                <valueColumn name="Discharge" parameterId="Discharge"/>
                <valueColumn name="WaterLevel" parameterId="WaterLevel"/>
            </table>
            <idMapId>IdExportHYDAT</idMapId>
            <exportMissingValueString>-9999.0</exportMissingValueString>
             <columnSeparator>,</columnSeparator>
            <decimalSeparator>.</decimalSeparator>
        </general>
		<timeSeriesSet>
			<moduleInstanceId>ImportWSC</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>QR.obs</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep id="$DAY_TIMESTEP$"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportWSC</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>HG.obs</parameterId>
			<locationId>$LOOP_LOCATION_ID$</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep id="$DAY_TIMESTEP$"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
    </export>
</timeSeriesExportRun>
