	<!-- Run MESH Model Historic -->
	<properties>
		<string key="STARTTIME" value="-30"/>
		<string key="ENDTIME" value="0"/>
		<string key="METEO_TIMESTEP" value="1"/>
		<string key="MODELBASINS" value="BowMESHBasins"/>
		<string key="BAT_FILE" value="run_mesh.bat"/>
		<string key="MODEL" value="Bow"/>
		<int key="TIMESHIFT" value="8"/>
	</properties>
	<!-- Run Historic Model to Create Updated State-->
	<activity>
		<runIndependent>true</runIndependent>
		<moduleInstanceId>BowMESHHistoric</moduleInstanceId>
		<moduleConfigFileName>MESHHistoricTemplate</moduleConfigFileName>
	</activity>
