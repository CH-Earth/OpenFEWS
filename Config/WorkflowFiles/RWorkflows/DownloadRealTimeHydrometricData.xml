<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/workflow.xsd" version="1.1">
<!--Run R script to extract recent hydrometric data-->
	<activity>
		<runIndependent>true</runIndependent>
		<moduleInstanceId>DownloadRealTimeHydrometricData</moduleInstanceId>
	</activity>
<!-- Import data into FEWS-->
	<activity>
		<runIndependent>true</runIndependent>
		<moduleInstanceId>ImportHYDATRecent</moduleInstanceId>
		<loopLocationSetId>DownloadHydroStations</loopLocationSetId>
	</activity>
</workflow>
